{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "npx prettier --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "check": "npm run lint && npm run format:check && tsc --noEmit",
    "prepare": "husky",
    "fix": "npm run lint:fix && npm run format",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-coverage": "npm run test-storybook:coverage && npm run generate-coverage-report",
    "e2e": "npx testplane gui --config ./testplane-run-config.cjs",
    "test-storybook": "vitest --project=storybook",
    "test-storybook:coverage": "test-storybook --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.3.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "classnames": "^2.5.1",
    "keep-react": "^1.6.1",
    "phosphor-react": "^1.4.1",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "react-swipeable": "^7.0.2",
    "swiper": "^11.2.2",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-onboarding": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/test-runner": "^0.17.0",
    "@swc/core": "^1.11.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/serviceworker": "^0.0.135",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.1.3",
    "chromatic": "^11.27.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.4",
    "glob": "^10.4.5",
    "globals": "^15.14.0",
    "html-reporter": "^10.16.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "lint-staged": "^15.5.0",
    "nyc": "^15.1.0",
    "playwright": "^1.51.0",
    "prettier": "^3.5.3",
    "storybook": "^8.6.4",
    "testplane": "^8.28.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
